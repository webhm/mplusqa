{
    "navigationFallback": {
        "rewrite": "index.html",
        "exclude": [
            "/images/*.{png,jpg,gif}",
            "/css/*"
        ]
    },
    "globalHeaders": {
        "Content-Type": "charset=UTF-8"
    },
    "mimeTypes": {
        ".html": "text/html",
        "custom": "text/html",
        ".json": "text/json",
        ".css": "text/css",
        ".js": "text/javascript"
    }
}