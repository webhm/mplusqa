{
    "navigationFallback": {
        "rewrite": "index.html",
        "exclude": [
            "/images/*.{png,jpg,gif}",
            "/css/*"
        ]
    },
    "responseOverrides": {
        "400": {
            "rewrite": "/"
        },
        "401": {
            "redirect": "/login",
            "statusCode": 302
        },
        "403": {
            "rewrite": "/"
        },
        "404": {
            "rewrite": "/index.html"
        }
    },
    "globalHeaders": {
        "Content-Type": "charset=UTF-8"
    },
    "mimeTypes": {
        ".html": "text/html",
        "custom": "text/html",
        ".json": "text/json",
        ".css": "text/css",
        ".js": "text/javascript"
    }
}