{
    "routes": [
        {
            "route": "/*",
            "rewrite": "/index.html"
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/images/*.{png,jpg,gif}",
            "/assets/dashforge/lib/jquery/*"
        ]
    },
    "mimeTypes": {
        ".html": "text/html",
        "custom": "text/html",
        ".json": "text/json",
        ".css": "text/css",
        ".js": "text/javascript"
    }
}